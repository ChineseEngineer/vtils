{
  "name": "vtils",
  "version": "3.0.1-beta.8",
  "description": "A business-oriented JavaScript utility library based on Lodash.",
  "keywords": [
    "util",
    "utils",
    "lodash"
  ],
  "homepage": "https://github.com/fjc0k/vtils#readme",
  "bugs": {
    "url": "https://github.com/fjc0k/vtils/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fjc0k/vtils.git"
  },
  "license": "MIT",
  "author": {
    "name": "Jay Fong",
    "email": "fjc0kb@gmail.com",
    "url": "https://github.com/fjc0k"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./_cjs/utils/index.js",
      "import": "./utils/index.js"
    },
    "./": {
      "require": "./_cjs/",
      "import": "./"
    },
    "./package.json": "./package.json"
  },
  "main": "_cjs/utils/index.js",
  "module": "utils/index.js",
  "types": "utils/index.d.ts",
  "scripts": {
    "build": "shx rm -rf ./lib && npm run generate-index && npm run build-esm && npm run build-cjs && npm run generate-package",
    "build-cjs": "tsc -p tsconfig.build-cjs.json && ts-node -T -P ./tsconfig.scripts.json ./scripts/fixCJSBuild.ts",
    "build-esm": "tsc -p tsconfig.build-esm.json",
    "generate-index": "vgis './src/**/index.ts'",
    "generate-package": "ts-node -T -P ./tsconfig.scripts.json ./scripts/generatePackage.ts",
    "generate-util": "ts-node -T -P ./tsconfig.scripts.json ./scripts/generateUtil.ts",
    "release": "standard-version -a && npm run release-pupu",
    "release-beta": "standard-version -a --release-as patch --prerelease beta && npm run release-pupu -- --tag next",
    "release-major": "standard-version -a --release-as major && npm run release-pupu",
    "release-major-beta": "standard-version -a --release-as major --prerelease beta && npm run release-pupu -- --tag next",
    "release-pupu": "npm run build && git push --follow-tags origin v3 && cd ./lib && npm publish",
    "test": "jest",
    "test-update-snapshot": "jest --updateSnapshot",
    "test-with-coverage": "jest --coverage",
    "test-with-coverage-open": "jest --coverage && open-cli ./coverage/lcov-report/index.html"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{css,less,scss,sass,html,htm,vue,yml,yaml,json,md}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@types/lodash-es": "^4.17.3",
    "date-fns": "^2.14.0",
    "lodash": "^4.17.15",
    "lodash-es": "^4.17.15",
    "ts-essentials": "^6.0.5",
    "tslib": "^2.0.0",
    "type-fest": "^0.15.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.1",
    "@types/prompts": "^2.0.8",
    "@types/standard-version": "^7.0.0",
    "codecov": "^3.7.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "execa": "^4.0.2",
    "fs-extra": "^9.0.0",
    "globby": "^11.0.0",
    "haoma": "^2.0.0-beta.2",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.6",
    "open-cli": "^6.0.1",
    "prettier": "^2.0.5",
    "prompts": "^2.3.2",
    "shx": "^0.3.2",
    "standard-version": "^8.0.0",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.3",
    "vscode-generate-index-standalone": "^1.3.0"
  },
  "engines": {
    "node": ">=13.2.0"
  }
}
