{
  "name": "vtils",
  "version": "1.0.2",
  "description": "一些常用的 JavaScript 工具函数。",
  "main": "lib/vtils.cjs.js",
  "module": "lib/vtils.es.js",
  "unpkg": "lib/vtils.min.js",
  "jsdelivr": "lib/vtils.min.js",
  "types": "lib/index.d.ts",
  "homepage": "https://github.com/fjc0k/vtils",
  "license": "MIT",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git@github.com:fjc0k/vtils.git"
  },
  "bugs": {
    "url": "https://github.com/fjc0k/vtils/issues"
  },
  "keywords": [
    "utils"
  ],
  "files": [
    "lib"
  ],
  "author": {
    "name": "Jay Fong",
    "email": "fjc0kb@gmail.com",
    "url": "https://github.com/fjc0k"
  },
  "scripts": {
    "docsOnly": "typedoc --ignoreCompilerErrors --excludeNotExported --excludePrivate --excludeProtected --out docs --theme minimal --mode file src/index.ts",
    "docs": "yarn docsOnly && gh-pages -d docs",
    "lint": "eslint --fix --ext .ts src/",
    "testOnly": "jest",
    "test": "jest --coverage",
    "build": "bili",
    "release": "standard-version -a && git push --follow-tags origin master && yarn build && yarn docs && npm publish"
  },
  "eslintConfig": {
    "root": true,
    "extends": "io"
  },
  "eslintIgnore": [
    "lib"
  ],
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.ts"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ]
  },
  "devDependencies": {
    "@types/jest": "^23.3.4",
    "@types/sinon": "^5.0.5",
    "bili": "^3.4.1",
    "codecov": "^3.1.0",
    "eslint": "^5.9.0",
    "eslint-config-io": "^0.5.0",
    "gh-pages": "^2.0.1",
    "jest": "^23.6.0",
    "moment": "^2.22.2",
    "rollup-plugin-typescript2": "^0.18.0",
    "standard-version": "^4.4.0",
    "ts-jest": "^23.10.4",
    "ts-node": "^8.0.2",
    "typed-we-app": "^1.7.0-update.6",
    "typedoc": "^0.14.2",
    "typescript": "^3.3.3333"
  }
}
