{
  "name": "vtils",
  "version": "3.0.1-beta.2",
  "description": "A business-oriented JavaScript utility library based on Lodash.",
  "keywords": [
    "util",
    "utils",
    "lodash"
  ],
  "homepage": "https://github.com/fjc0k/vtils#readme",
  "bugs": {
    "url": "https://github.com/fjc0k/vtils/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fjc0k/vtils.git"
  },
  "license": "MIT",
  "author": {
    "name": "Jay Fong",
    "email": "fjc0kb@gmail.com",
    "url": "https://github.com/fjc0k"
  },
  "sideEffects": false,
  "main": "lib/index.js",
  "jsdelivr": "lib/index.umd.min.js",
  "unpkg": "lib/index.umd.min.js",
  "module": "lib/index.esm.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "prebuild": "shx rm -rf ./node_modules/.cache/build && tsc -p tsconfig.build.json && npm run generate-index",
    "build": "shx rm -rf ./lib && rollup -c",
    "generate-index": "vgis ./src/index.ts",
    "generate-util": "ts-node -T -P ./tsconfig.scripts.json ./scripts/generateUtil.ts",
    "prepublishOnly": "npm run build",
    "release": "standard-version -a && npm run release-pupu",
    "release-beta": "standard-version -a --release-as patch --prerelease beta && npm run release-pupu -- --tag next",
    "release-major": "standard-version -a --release-as major && npm run release-pupu",
    "release-major-beta": "standard-version -a --release-as major --prerelease beta && npm run release-pupu -- --tag next",
    "release-pupu": "git push --follow-tags origin v3 && npm publish",
    "test": "jest",
    "test-update-snapshot": "jest --updateSnapshot",
    "test-with-coverage": "jest --coverage",
    "test-with-coverage-open": "jest --coverage && open-cli ./coverage/lcov-report/index.html"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{css,less,scss,sass,html,htm,vue,yml,yaml,json,md}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@types/lodash-es": "^4.17.3"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@types/fs-extra": "^9.0.1",
    "@types/prompts": "^2.0.8",
    "@types/standard-version": "^7.0.0",
    "codecov": "^3.7.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "execa": "^4.0.2",
    "fs-extra": "^9.0.0",
    "haoma": "^2.0.0-beta.1",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.6",
    "lodash-es": "^4.17.15",
    "open-cli": "^6.0.1",
    "prettier": "^2.0.5",
    "prompts": "^2.3.2",
    "rollup": "^2.11.0",
    "rollup-plugin-dts": "^1.4.7",
    "rollup-plugin-terser": "^6.1.0",
    "shx": "^0.3.2",
    "standard-version": "^8.0.0",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.3",
    "vscode-generate-index-standalone": "^1.3.0"
  }
}
