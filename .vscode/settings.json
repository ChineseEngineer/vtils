{
  "eslint.enable": true,
  "eslint.nodePath": "./node_modules",
  "eslint.autoFixOnSave": true,
  "eslint.packageManager": "yarn",
  "eslint.run": "onSave",
  "eslint.options": {
    "ignorePath": ".gitignore",
    "cache": true,
    "cacheLocation": "./node_modules/.cache/eslint"
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    {
      "language": "typescript",
      "autoFix": true
    },
    {
      "language": "typescriptreact",
      "autoFix": true
    }
  ],
}
